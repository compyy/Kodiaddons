<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.siasat-pk"
       name="siasat-pk"
       version="0.2"
       provider-name="Yasir">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
	<import addon="script.module.urlresolver" version="1.0.0"/>
	<import addon="plugin.video.youtube" version="2.9.2"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="addon.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">www.siasat.pk</summary>
    <description lang="en">Watch Vidoes uploaded to siasat.pk</description>
    <platform>all</platform>
  </extension>
</addon>
<addon id="repository.yasir" name="Yasirs Addon Repository" version="0.3" provider-name="Yasir">
	<extension point="xbmc.addon.repository" name="Yasirs Addon Repository">
    <dir>
            <info compressed="false">https://raw.github.com/compyy/Kodiaddons/master/addons.xml</info>
            <checksum>https://raw.github.com/compyy/Kodiaddons/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.github.com/compyy/Kodiaddons/master/zips</datadir>
    </dir>
    
    <info compressed="false">https://raw.github.com/compyy/Kodiaddons/master/addons.xml</info>
    <checksum>https://raw.github.com/compyy/Kodiaddons/master/addons.xml.md5</checksum>
    <datadir zip="true">https://raw.github.com/compyy/Kodiaddons/master/zips</datadir>

	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Addons related to Pakistani videos</summary>
		<description>Repositry for my addons</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
